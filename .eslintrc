{
  "extends": [
    "standard",
    "standard-react"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-curly-spacing": [
      2,
      "always",
      {
        "spacing": {
          "objectLiterals": "never"
        }
      }
    ],
    "react/prop-types": 0,
    "react/no-unused-prop-types": 0,
    "react/self-closing-comp": [
      "error",
      {
        "component": true,
        "html": true
      }
    ],
    "react/jsx-boolean-value": 0,
    "react/jsx-no-bind": 0,
    "no-throw-literal": 2,
    "no-unused-vars": 0,
    "no-useless-constructor": 0,
    "no-unneeded-ternary": 0,
    "no-return-assign": 2,
    "no-extra-parens": 0,
    "no-unused-expressions": 0,
    "no-use-before-define": 0,
    "space-before-function-paren": [
      2,
      {
        "anonymous": "never",
        "named": "always"
      }
    ],
    "space-in-parens": [
      "error",
      "never"
    ],
    "arrow-parens": [
      2,
      "as-needed",
      {
        "requireForBlockBody": true
      }
    ],
    "one-var": [
      "error",
      {
        "initialized": "never"
      }
    ],
    "object-property-newline": 0,
    "camelcase": 0,
    "spaced-comment": ["error", "always"],
    "yoda": 0,
    "curly": 0
  },
  "globals": {
    $: true
  }
}